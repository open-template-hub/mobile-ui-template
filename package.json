{
  "name": "mobile-ui-template",
  "description": "Mobile UI Template for generic usage in React Native",
  "version": "2.0.2",
  "license": "MIT",
  "scripts": {
    "android-debug": "npx react-native run-android --variant=debug",
    "android-clean": "cd android && gradlew clean && gradlew cleanBuildCache && cd ..",
    "android-release": "npx react-native run-android --variant=release",
    "build": "npm install && npm audit fix && npx react-native init mobileuitemplate && react-native link && mv mobileuitemplate/ios ios && mv mobileuitemplate/android android && rm -rf mobileuitemplate && cp android-backup/app android && react-native link",
    "build-windows": "npm install && npm audit fix && npx react-native init mobileuitemplate && react-native link && move mobileuitemplate/ios ios && move mobileuitemplate/android android && Rmdir /S /Q mobileuitemplate && xcopy /E /I /Y \"android-backup/app\" \"android/app\" && react-native link",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@fortawesome/react-native-fontawesome": "^0.2.6",
    "@invertase/react-native-apple-authentication": "^2.1.2",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/admob": "^10.3.1",
    "@react-native-firebase/app": "^12.9.0",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/drawer": "^6.1.6",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@types/i18n-js": "^3.0.3",
    "@types/react-native-fbsdk": "^1.1.1",
    "@types/react-native-google-signin": "^1.0.2",
    "axios": "^0.21.0",
    "i18n-js": "^3.8.0",
    "react": "16.13.1",
    "react-native": ">=0.64.1",
    "react-native-animated-spinkit": "^1.4.2",
    "react-native-fbsdk": "3.0.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-google-signin": "^2.1.1",
    "react-native-iap": "^7.5.0",
    "react-native-image-picker": "^3.1.4",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.2.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.0",
    "react-native-simple-toast": "^1.1.3",
    "react-native-svg": "^12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.37",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^27.2.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "mobile",
    "mobile ui",
    "ui",
    "android",
    "ios",
    "ui template",
    "app",
    "typescript",
    "template",
    "open template hub"
  ]
}
